{
  "family": "blacklist-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::416825837619:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "blacklist",
      "image": "416825837619.dkr.ecr.us-east-1.amazonaws.com/blacklist-service:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
